generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model about {
  id      String @id @default(auto()) @map("_id") @db.ObjectId
  content String
  title   String
  type    String
}

model contact {
  id String @id @default(auto()) @map("_id") @db.ObjectId
}

model experiences {
  id       String @id @default(auto()) @map("_id") @db.ObjectId
  content  String
  image    String
  position Int
  title    String
  type     String
}

model home {
  id      String @id @default(auto()) @map("_id") @db.ObjectId
  content String
  title   String
  type    String
}
